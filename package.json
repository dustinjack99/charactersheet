{
  "name": "charactersheet",
  "version": "1.0.0",
  "description": "Character Sheet Discord Interface",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "dev": "webpack-dev-server --mode development --open --hot",
    "build": "npm run check && webpack --mode production",
    "check": "tsc",
    "format:frontend": "prettier --write frontend",
    "clean": "rm -rf dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinjack99/charactersheet.git"
  },
  "author": "dustinjack99, Giesch",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "bugs": {
    "url": "https://github.com/dustinjack99/charactersheet/issues"
  },
  "homepage": "https://github.com/dustinjack99/charactersheet#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sequelize": "^5.21.6"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.31",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^4.0.3",
    "prettier": "2.0.4",
    "sequelize-cli": "^5.5.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
